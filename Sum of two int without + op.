#include <iostream>
using namespace std;

int getSum(int a, int b) {
    
    for(int i = 0 ; i < b ; i++)
    {
        a++;
    }
   return a;
}

int main() {
    int a, b;
    cout << "Enter two integers: ";
    cin >> a >> b;

    int sum = getSum(a, b);

    cout << "Sum: " << sum << endl;

    return 0;
}
